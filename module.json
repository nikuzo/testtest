    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "compatibility": {
                    "minimum": "3.2.0",
                    "verified": "3.3.1",
                    "maximum": "4.0"
                }
            }
        ],
        "modules": []
    },
    "description": "",
